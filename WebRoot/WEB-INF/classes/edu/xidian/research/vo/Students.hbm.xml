<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	
	<class name="edu.xidian.research.vo.Students" table="students">
		<id name="ID" column="ID" type="int"></id>
		<property name="stuname" column="stuname" type="string"></property>
		<property name="stunum" column="stunum" type="string"></property>
		<property name="cardid" column="cardid" type="string"></property>
	</class>

</hibernate-mapping>